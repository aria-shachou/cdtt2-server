    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "float"
    },
    "company": {
      "type": "enumeration",
      "enum": [
        "ikea",
        "caressa",
        "marcos",
        "liddy"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "office",
        "living room",
        "kitchen",
        "bedroom",
        "dining",
        "kid"
      ]
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
